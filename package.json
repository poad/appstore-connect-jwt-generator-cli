{
  "name": "appstore-connect-jwt-generator-cli",
  "version": "1.0.2",
  "author": "poad",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/poad/appstore-connect-jwt-generator-cli#readme",
  "bin": {
    "jwt-gen": "bin/cli.mjs"
  },
  "main": "bin/cli.mjs",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/appstore-connect-jwt-generator-cli.git"
  },
  "keywords": [
    "jwt",
    "appstore",
    "cli"
  ],
  "scripts": {
    "test": "jest --coverage --passWithNoTests",
    "clean": "rimraf bin/main*",
    "compile": "swc src -d bin && mv bin/cli.js bin/cli.mjs && mv bin/main.js bin/main.mjs",
    "build": "yarn clean && yarn compile",
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/main.ts",
    "lint": "eslint src/**/*.ts src/*.ts",
    "all": "yarn build && yarn lint && yarn test"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.133",
    "@swc/helpers": "^0.3.2",
    "@swc/jest": "^0.2.17",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-jest": "^27.4.6",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "swc-loader": "^0.1.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "^1.1.6",
    "arg": "^5.0.1",
    "chalk": "^5.0.0",
    "chalk-template": "^0.3.1",
    "log4js": "^6.4.1"
  },
  "files": [
    "package.json",
    "bin/*"
  ],
  "exports": {
    ".": {
      "node": "./bin/cli.js",
      "default": "./bin/cli.js"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
