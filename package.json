{
  "name": "appstore-connect-jwt-generator-cli",
  "version": "1.0.0",
  "author": "poad",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/poad/appstore-connect-jwt-generator-cli#readme",
  "bin": {
    "jwt-gen": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/appstore-connect-jwt-generator-cli.git"
  },
  "keywords": [
    "jwt",
    "appstore",
    "cli"
  ],
  "type": "module",
  "scripts": {
    "prepare": "yarn build",
    "test": "jest --coverage --passWithNoTests",
    "clean": "rm -rf bin/main*",
    "compile": "swc src/main.ts -o bin/main.js",
    "build": "yarn clean && yarn compile",
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/main.ts",
    "lint": "eslint src/**/*.ts src/*.ts",
    "all": "yarn build && yarn lint && yarn test"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.95",
    "@swc/helpers": "^0.2.13",
    "@swc/jest": "^0.2.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.2.5",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-flowtype": "^6.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^25.0.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "swc-loader": "^0.1.15",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "^1.0.0",
    "arg": "^5.0.1",
    "chalk": "^4.1.2",
    "log4js": "^6.3.0"
  },
  "files": [
    "package.json",
    "bin/*"
  ],
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
