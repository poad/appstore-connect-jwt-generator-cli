{
  "name": "appstore-connect-jwt-generator-cli",
  "version": "1.0.3",
  "author": "poad",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/poad/appstore-connect-jwt-generator-cli#readme",
  "bin": {
    "jwt-gen": "bin/index.mjs"
  },
  "main": "bin/cli.mjs",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/appstore-connect-jwt-generator-cli.git"
  },
  "keywords": [
    "jwt",
    "appstore",
    "cli"
  ],
  "scripts": {
    "test": "jest --coverage --passWithNoTests",
    "clean": "rimraf bin/main*",
    "compile": "swc src -d bin && mv bin/cli.js bin/cli.mjs && mv bin/main.js bin/main.mjs",
    "build": "yarn clean && yarn compile",
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/main.ts",
    "package": "ncc build --source-map --license licenses.txt -m -q -o bin",
    "lint": "eslint src/**/*.ts src/*.ts",
    "all": "yarn build && yarn package && yarn lint && yarn test"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.219",
    "@swc/helpers": "^0.4.3",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vercel/ncc": "^0.34.0",
    "babel-jest": "^28",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "*",
    "arg": "^5.0.2",
    "chalk": "^5.0.1",
    "chalk-template": "^0.4.0",
    "log4js": "^6.6.1"
  },
  "files": [
    "package.json",
    "bin/*"
  ],
  "exports": {
    ".": {
      "node": "./bin/index.js",
      "default": "./bin/index.js"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}
