{
  "name": "appstore-connect-jwt-generator-cli",
  "version": "1.0.6",
  "author": "poad",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/poad/appstore-connect-jwt-generator-cli#readme",
  "bin": {
    "jwt-gen": "bin/index.mjs"
  },
  "main": "bin/cli.mjs",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/appstore-connect-jwt-generator-cli.git"
  },
  "keywords": [
    "jwt",
    "appstore",
    "cli"
  ],
  "scripts": {
    "test": "jest --coverage --passWithNoTests",
    "clean": "rimraf bin/main*",
    "compile": "swc src -d bin && mv bin/cli.js bin/cli.mjs && mv bin/main.js bin/main.mjs",
    "build": "pnpm clean && pnpm compile",
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/main.ts",
    "package": "ncc build --source-map --license licenses.txt -m -q -o bin",
    "lint": "eslint src/**/*.ts src/*.ts",
    "all": "pnpm build && pnpm package && pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.9.3",
    "@swc/helpers": "^0.5.15",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/ncc": "^0.36.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^5.0.10",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "^1.1.15",
    "arg": "^5.0.2",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "log4js": "^6.9.1"
  },
  "files": [
    "package.json",
    "bin/*"
  ],
  "exports": {
    ".": {
      "node": "./bin/index.js",
      "default": "./bin/index.js"
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.1",
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387",
  "pnpm": {
    "overrides": {
      "cross-spawn@<7.0.5": ">=7.0.5",
      "webpack": ">=5.94.0"
    }
  }
}
