{
  "name": "appstore-connect-jwt-generator-core",
  "version": "1.1.7",
  "author": "poad",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/poad/appstore-connect-jwt-generator-core#readme",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/appstore-connect-jwt-generator-core.git"
  },
  "keywords": [
    "jwt",
    "appstore"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "@types/index.d.ts",
  "main": "./dist/index.mjs",
  "scripts": {
    "prepare": "yarn build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --passWithNoTests",
    "clean": "rimraf dist",
    "cjs": "tsc -p tsconfig-cjs.json",
    "esm": "tsc -p tsconfig-esm.json",
    "types": "tsc -p tsconfig-types.json",
    "build": "yarn clean && yarn cjs && yarn esm && yarn types",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/esm/**/*.mts src/esm/*.mts src/cjs/**/*.cts src/cjs/*.cts src/*.ts"
  },
  "devDependencies": {
    "@swc/core": "^1.2.151",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chokidar": "^3.5.3",
    "enhanced-resolve": "^5.9.2",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.4.2",
    "regenerator-runtime": "^0.13.9"
  },
  "engines": {
    "node": ">=14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist",
    "@types"
  ]
}
