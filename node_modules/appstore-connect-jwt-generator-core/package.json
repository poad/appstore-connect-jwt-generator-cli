{
  "name": "appstore-connect-jwt-generator-core",
  "version": "1.1.13",
  "author": "poad",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/poad/appstore-connect-jwt-generator-core#readme",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/appstore-connect-jwt-generator-core.git"
  },
  "keywords": [
    "jwt",
    "appstore"
  ],
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./esm/index.mjs"
    }
  },
  "module": "./esm/index.mjs",
  "types": "@types/index.d.ts",
  "main": "./lib/index.cjs",
  "scripts": {
    "clean": "rimraf dist",
    "cjs": "swc --config-file .swcrc-cjs --delete-dir-on-start --extensions .cts src/cjs -o lib/index.cjs",
    "esm": "swc --config-file .swcrc-esm --delete-dir-on-start --extensions .mts src/esm -o esm/index.mjs",
    "types": "tsc -p tsconfig-types.json",
    "build": "yarn clean && yarn cjs && yarn esm && yarn types",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/esm/**/*.mts src/esm/*.mts src/cjs/**/*.cts src/cjs/*.cts src/*.ts",
    "test": "jest --coverage --passWithNoTests"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.179",
    "@swc/jest": "^0.2.21",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chokidar": "^3.5.3",
    "enhanced-resolve": "^5.9.3",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.4.6",
    "regenerator-runtime": "^0.13.9"
  },
  "engines": {
    "node": ">=14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}
